{
  "name": "lyrid-js-sdk",
  "version": "1.0.2",
  "description": "A NPM package that consume Lyrid functions.",
  "main": "src/index.js",
  "scripts": {
    "build": "babel ./src -d ./lib && browserify src/index.js --s Lyrid > ./dist/lyrid.js",
    "lint": "eslint ./src",
    "test": "nyc mocha --require babel-core/register",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LyridInc/js-sdk.git"
  },
  "keywords": [
    "npm",
    "node",
    "lyrid",
    "javascript"
  ],
  "author": "Darian UNG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LyridInc/js-sdk/issues"
  },
  "homepage": "https://github.com/LyridInc/js-sdk#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
